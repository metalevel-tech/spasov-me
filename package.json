{
	"name": "spasov-me",
	"version": "3.3.0",
	"private": true,
	"scripts": {
		"env-pull": "pnpm dlx vercel env pull",
		"env-inject": "dotenv -e ./.env.local --",
		"check-branch-name": "chmod 755 githooks/check_branch_name.sh && bash githooks/check_branch_name.sh",
		"generate-icon-map": "ts-node --skip-project scripts/generate-icon-map.ts",
		"purge-cache": "rm -rf .next",
		"dev-purge": "pnpm purge-cache && next dev",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"start0": "next start -p 3000 --hostname 0.0.0.0",
		"start1": "next start -p 3001 --hostname 0.0.0.0",
		"lint": "next lint",
		"prepare": "husky install",
		"pre-commit": "pnpm run check-branch-name && lint-staged",
		"git-push-u": "git push origin $(git branch | sed 's/^..//') -u"
	},
	"lint-staged": {
		"*.{ts,tsx}": "eslint --fix",
		"*.{css,scss}": "prettier --write",
		"*.css": "stylelint --fix",
		"*.scss": "stylelint"
	},
